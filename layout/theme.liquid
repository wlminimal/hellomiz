<!-- Product Selector -->
{% if template contains 'product' %}
<script type="text/javascript">
(function($) {
  var onVariantSelected = function(variant, selector) {
    // Update price display.
    var formattedPrice = Shopify.formatMoney(variant.price, "");
    $('#product-price').text(formattedPrice);
    // Enable or disable the Add to Cart button.
    $('#add-to-cart').attr('disabled', variant.available ? null : 'disabled');
    if(variant.available == false) {
      $('#add-to-cart').val('Sold Out');
      $('#product-price').text('SOLD OUT');
    }else {
      $('#add-to-cart').val('Add to Basket');
    };
    // Update the featured image.
    if(variant.featured_image) {
      var sizedImageUrl = Shopify.Image.getSizedImageUrl(variant.featured_image.src, 'master');
      var scaledImageUrl = Shopify.Image.getSizedImageUrl(sizedImageUrl, '1024x1024@2x');
      $('#featured-image').attr('src', sizedImageUrl);
      $('#featured-image').attr('data-zoom', scaledImageUrl);
      
    };

    console.log("From onVariantSelected");
    console.log(variant.id);

  };
  // Instantiate the option selectors.
  var optionSelectors = new Shopify.OptionSelectors("product-select", { product: {{ product | json }}, onVariantSelected: onVariantSelected, enableHistoryState: true });
  // Create an event listener to set a specific variant when clicking on image thumbnails.
  $(document).on('click', '[data-change-variant]', function(e) {
    e.preventDefault();
    optionSelectors.selectVariant($(this).data('changeVariant'));
  });
  // Create an event listener to set a specific variant option when clicking on swatches.(Size)
  $(document).on('click', '[data-change-option]', function(e) {
    e.preventDefault();
    $('[data-change-option]').css('border-color', '');
    $(this).css('border-color', '#FF9178');
    var optionIndex = parseInt($(this).data('changeOption')), optionValue = $(this).data('changeValue');
    if(optionSelectors.optionExistInSelect(optionSelectors.selectors[optionIndex].element, optionValue)) {
      $(optionSelectors.selectors[optionIndex].element).val(optionValue).trigger('change');
    }
  });

  // Create an event listener to set a specific variant option when clicking on swatches.(Color)
  $(document).on('click', '[data-change-option2]', function(e) {
    e.preventDefault();
    
    $('[data-change-option2]').css('border-color', '');
    $(this).css('border-color', '#FF9178');
    var optionIndex = parseInt($(this).data('changeOption2')), optionValue = $(this).data('changeValue2');
    if(optionSelectors.optionExistInSelect(optionSelectors.selectors[optionIndex].element, optionValue)) {
      $(optionSelectors.selectors[optionIndex].element).val(optionValue).trigger('change');
    }
    // move slider
    var variantId = $(this).data('variantId');
    console.log(variantId);

    console.log(optionValue);

    //$('.slider-nav').slick('slickUnfilter');
    //$('.slider-for').slick('slickUnfilter');
    //$('.slider-nav').slick('slickFilter', '.color_' + optionValue);
    
    console.log(optionIndex);
    $('.slider-for').slick('slickGoTo', 0);

    $('.zoomImg').remove();
    initZoom();
     
  });

  // Show side cart after request complete
  $(document).on('cart.requestComplete', function(event, cart) {
    console.log("Cart Request Complete, Show Side Cart");
    $('#side-cart').css('right', '0px');
  });

  $(document).on('cart.ready', function(event, cart) {
    console.log("Cart is ready");
  });

  $(document).on('cart.requestStarted', function(event, cart) {
    console.log("Cart is requestStarted");
  });
  
}(jQuery));
</script>
